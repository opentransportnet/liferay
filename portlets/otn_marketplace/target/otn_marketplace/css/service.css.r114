/* Marketplace customizations */

.marketplace {
/* 	font: 100 15px/1.5 myriad-pro, Arial, Helvetica, sans-serif; */
	font-size: 13px !important;
	width:100%;	
}

.marketplace a {
	color: #1085A0;
	font-weight: bold;
}

.logo{
	width:64px; 
	height:64px;
}

.serviceCategory {
	font-weight: bold;
	text-decoration: underline;
	font-size: 15px !important;	
}

.serviceOwner {
	font-style: italic;
	font-size: 14px !important;	
}

.serviceDetailsOwner {
	font-style: italic;
	font-size: 16px !important;	
}

.serviceTitle {
	color: #1085A0;
	font-weight: bold;
}

.serviceDetailsTitle {
	color: #1085A0;
	font-weight: bold;
	font-size: 18px !important;	
}


#serviceDetailsDiv {
    padding-bottom: 21px;
}

.searchField {
	
	border-color: #AAA;
    border-width: 0 0 2px 0 !important;
    /* font-size: 1.5em; */
    padding: 0 0 0 25px !important;
}

.searchButton {
	
	-moz-border-radius:10px;
	-webkit-border-radius:10px;
	border-radius:10px;
	cursor:pointer;

	text-decoration:none;	
}

.price {
	font-size: .7em;
	padding: 1px 5px;
	border:1px solid #939598;
	color: #939598;
	/* cursor:pointer; */
	text-decoration:none;
	border-radius:5px;
	text-align:center;
	vertical-align:top;
	height:auto;
	display:inline-block;
	margin-bottom: 34px;
	
}

.carousel-item {
	width:700px; 
	height:250px;	
}

hr {
	height:1px;
}

